            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import java.util.regex.*;

String responseData = prev.getResponseDataAsString();

String regex = &quot;=(.*?)(?=\&quot;)&quot;;

// Compile the regular expression into a Pattern object
Pattern pattern = Pattern.compile(regex);

// Create a Matcher object to find matches in the input string
Matcher matcher = pattern.matcher(responseData);

if (matcher.find()) {

    String matchedValue = matcher.group(1);
    log.info(&quot;Matched Value: &quot; + matchedValue);
    // You can use matchedValue in your script as needed
    vars.put(&quot;AUTH_CODE&quot;, matchedValue); // Store the value in a JMeter variable if needed
} else {
    log.error(&quot;No match found.&quot;);
}</stringProp>
